[submodule "pico-sdk"]
	path = lib/pico-sdk
	url = git@github.com:raspberrypi/pico-sdk.git
    shallow = true
    branch = 2.0.0
    ignore = dirty
[submodule "freertos"]
	path = lib/freertos
	url = git@github.com:FreeRTOS/FreeRTOS-Kernel.git
    shallow = true
    branch = V11.1.0
    ignore = dirty
[submodule "unity"]
	path = lib/unity
	url = git@github.com:ThrowTheSwitch/Unity.git
    ignore = dirty
[submodule "extern/FreeRTOS"]
	path = extern/FreeRTOS
	url = git@github.com:FreeRTOS/FreeRTOS.git
[submodule "lib/unity"]
	path = lib/unity
	url = https://github.com/ThrowTheSwitch/Unity.git
[submodule "lib/pico-examples"]
	path = lib/pico-examples
	url = https://github.com/raspberrypi/pico-examples.git
